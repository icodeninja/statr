{
  "manifest_version":2,
  "name":"Statr",
  "description":"Display more comprehensive ESPN Fantasy Baseball stats with a Chrome extension lol.",
  "version":"0.1.0",
  "browser_action":{
    "default_icon":"assets/logo.png",
    "default_popup":"assets/main.html",
    "default_title":"Statr"
  },
  "icons":{
    "512":"assets/logo.png",
    "128":"assets/logo128.png",
    "48":"assets/logo48.png"
  },
  "permissions":[
    "activeTab"
  ]
}
